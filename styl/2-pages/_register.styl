#register
  .button-container
    position: relative
    height: 15vh
    width: 100%
    display: flex
    border-bottom: 3px solid $white
    a
      text-align: center
      font-size: 2em
      line-height: 1
      margin: 0
      color: $white
      width: 50%
      padding: calc(15vh / 2 - 0.5em)
      background-color: $bg 
      text-decoration: none
      transition: all 300ms ease-in-out
      &:hover
        background-color: $bg-strip
      &:first-child
        border-right: 1px solid rgba($white, 0.2) 
      &.active
        background-color: $bg-strip
  .frame-container
    height: 85vh
    position: absolute
    bottom: 0
    left: 0
    right: 0
    #ladies
      display: none
